  `comid` smallint(6) DEFAULT '0' COMMENT '对应单位id',
  `type` tinyint(1) DEFAULT '0' COMMENT '0收入,1支出',
  `money` decimal(10,2) DEFAULT '0.00' COMMENT '金额',
